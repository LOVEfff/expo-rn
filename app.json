{
	"expo": {
		"name": "expo-rn",
		"slug": "expo-rn",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"newArchEnabled": true,
		"scheme": "music-player",
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.music.player",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typeRoutes": true,
			"tsconfigpaths": true
		}
	}
}
